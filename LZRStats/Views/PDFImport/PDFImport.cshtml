

@{
    ViewBag.Title = "PDFImport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>Statistics import</h2>
<div class="row">
    @using (Html.BeginForm("PDFImport", "PDFImport", FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="col-sm-4">
            <input type="file" name="file" accept=".docx" />
        </div>
        <div class="col-sm-4">
            <input type="submit" value="Import" />
        </div>
    }
</div>